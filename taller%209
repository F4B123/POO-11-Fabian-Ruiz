(function() {

const array = [ 1, 30, 4, 21, 100000 ];

function desCompare( a, b ) {
	return b - a;
}

array.sort( desCompare );
console.log( array );	    //arreglo de mayor a menor

for ( let x = 4; x >= 0; x--){
    console.log( array[x]);     //recorrer el arreglo desde el final hacia el principio
}


})();
